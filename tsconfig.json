{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "@element/*": [
        "app/elements/*"
      ],
      "@layout/*": [
        "app/components/layouts/*"
      ],
      "@module/*": [
        "app/components/modules/*"
      ],
      "@template/*": [
        "app/components/templates/*"
      ],
      "@constant/*": [
        "app/constants/*"
      ],
      "@context/*": [
        "app/context/*"
      ],
      "@hook/*": [
        "app/hooks/*"
      ],
      "@util/*": [
        "app/utils/*"
      ],
      "@api/*": [
        "app/api/*"
      ],
      "@styles/*": [
        "app/styles/*"
      ],
      "@stores/*": [
        "app/stores/*"
      ],
      "@libs/*": [
        "app/libs/*"
      ],
      "@types/*": [
        "app/types/*"
      ]
    },
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "types": [
      "cypress"
    ],
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
